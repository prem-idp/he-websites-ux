{
  "name": "my-turborepo",
  "private": true,
  "version": "0.1.0",
  "workspaces": [
    "apps/whatuni",
    "apps/pgs"
  ],
  "type": "module",
  "bin": {
    "cdk": "bin/cdk.js"
  },
  "scripts": {
    "build": "npm run build --workspace=whatuni",
    "start": "npm run start --workspace=whatuni",
    "dev": "npm run dev --workspace=whatuni",
    "lint": "eslint .",
    "test": "jest --watchAll",
    "test:coverage": "jest --coverage",
    "dev:pgs": "npm run dev --workspace=pgs",
    "build:pgs": "npm run build --workspace=pgs",
    "start:pgs": "npm run start --workspace=pgs",
    "open-next:pgs": "cd apps/pgs && open-next build",
    "cdk-synth:pgs": "cd apps/pgs/cdk && npm install && cdk synth --build",
    "cdk-diff:pgs": "cd apps/pgs/cdk && cdk diff",
    "cdk-deploy:pgs": "cd apps/pgs/cdk && cdk deploy --all --require-approval never --ci",
    "dev:whatuni": "npm run dev --workspace=whatuni",
    "build:whatuni": "npm run build --workspace=whatuni",
    "start:whatuni": "npm run start --workspace=whatuni",
    "open-next:whatuni": "cd apps/whatuni && echo $PWD && open-next build",
    "cdk-synth:whatuni": "npm install && cdk synth --build",
    "cdk-diff:whatuni": "cdk diff",
    "cdk-deploy:whatuni": "cdk deploy --all --require-approval never --ci",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
